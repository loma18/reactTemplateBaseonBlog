{
	"presets": [
		[
			"env",
			{
				"targets": {
					"browsers": [
						"> 1%",
						"last 2 versions",
						"safari >= 7",
						"not ie <= 10"
					]
				},
				"useBuiltIns": true
			}
		],
		"es2015",
		"react",
		"stage-2"
	],
	"plugins": [
		"transform-runtime",
		"transform-decorators-legacy",
		[
			"import",
			{
				"libraryName": "antd",
				"libraryDirectory": "es",
				"style": true // `style: true` 会加载 less 文件
			}
		]
	]
}